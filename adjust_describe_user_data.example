<powershell>
Import-Module AWSPowerShell
Set-AWSCredential -AccessKey ${AWS_ACCESS_KEY_ID} -SecretKey ${AWS_SECRET_ACCESS_KEY} -StoreAs default
Copy-S3Object -Region ${S3_BUCKET_REGION} -BucketName ${S3_BUCKET_NAME} -KeyPrefix ${S3_BUCKET_KEY} -LocalFolder C:\s3-files\

# Install IIS before these run
Invoke-Expression -Command "C:\s3-files\describe_site.ps1" # config for describe site. see: describe_site.ps1.example
Invoke-Expression -Command "C:\s3-files\adjust.ps1" # generated by driver
Invoke-Expression -Command "C:\s3-files\describe.ps1"   # generated by driver
</powershell>
