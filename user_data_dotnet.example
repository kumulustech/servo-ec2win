<powershell>
Import-Module AWSPowerShell
Copy-S3Object -Region ${S3_BUCKET_REGION} -BucketName ${S3_BUCKET_NAME} -KeyPrefix ${S3_BUCKET_KEY} -LocalFolder C:\s3-files\

# Install IIS before these run
Invoke-Expression -Command "C:\s3-files\describe_site.ps1" # config for describe site in IIS. see: describe_site.ps1.example
Invoke-Expression -Command "C:\s3-files\adjust.ps1" # generated by driver
Invoke-Expression -Command "C:\s3-files\describe.ps1"   # script generated by dotnet encoder's encode_describe. see: describe_endpoint_dotnet.ps1.example
</powershell>
